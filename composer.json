{
    "name": "osflab/test",
    "description": "OSF simple unit test component",
    "version": "3.0.1-dev",
    "type": "library",
    "homepage": "https://github.com/osflab/test",
    "license": "AGPL-3.0-or-later",
    "authors": [
        {
            "name": "Guillaume Pon√ßon",
            "email": "guillaume.poncon@openstates.com"
        }
    ],
    "support": {
